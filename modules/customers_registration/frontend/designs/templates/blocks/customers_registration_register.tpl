
<section class="ftco-section" style="background-color: rgb(85, 85, 85);">
    <div class="page-wrap">
        <img src="/web/pictures/registration-form.jpg" style="max-width: 100%;width: auto;margin: 0 auto;display: block;"/>
        <div class="container" style="color: #fff;">  
            <h1 style="color: rgb(224, 198, 0);text-align: center;font-weight: bold;">{__('RSVP for Graphex Technology October Grand Opening')}</h1>
            <p>
                {__('Dear Friends,<br/>
                        Please join us on Saturday, October 17th, 2020, at 7:00 PM <br/>
                         in this magnitude celebration; as we introduce this revolutionary<br/>
                         movement toward greatness in our ever‒evolving virtual technological world.<br/>
                         Please RSVP by Thursday, October 15th, 2020. <br/>
                         We are looking forward to hearing from you. <br/>
                        ')}
            </p>
            <div class="row">
                <div class="col-md-6" style="padding-bottom: 30px;">
                    <input type="text" class="form-control"/>
                    <span>{__('First Name *')}</span>
                </div>
                <div class="col-md-6" style="padding-bottom: 30px;">
                    <input type="text" class="form-control"/>
                    <span>{__('Last Name *')}</span>
                </div>
                <div class="col-md-12" style="padding-bottom: 30px;">
                    <span>{__('Please Enter Your Email Address *')}</span>
                    <input type="text" class="form-control"/>
                </div>
                <div class="col-md-12" style="padding-bottom: 30px;">
                    <span>{__('Please Enter Your Phone Number *')}</span>
                    <input type="text" class="form-control"/>
                </div>
                <div class="col-md-12" style="padding-bottom: 30px;">
                    <span>{__('Please Indicate Your Race/Ethnicity *')}</span>
                    <input type="text" class="form-control"/>
                </div>
                <div class="col-md-12" style="padding-bottom: 30px;">
                    <div>{__('Please Indicate If You Are Attending As Any Of The Following *')}</div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="student" value="student">
                        <label class="form-check-label" for="student">{__('Student')}</label>
                    </div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="school-faculty" value="school-faculty">
                        <label class="form-check-label" for="school-faculty">{__('School Faculty')}</label>
                    </div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="employee" value="employee">
                        <label class="form-check-label" for="employee">{__('Freelancer')}</label>
                    </div><br/>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="employer" value="employer">
                        <label class="form-check-label" for="employee">{__('Employer')}</label>
                    </div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="enptrepreneur" value="enptrepreneur">
                        <label class="form-check-label" for="enptrepreneur">{__('Enptrepreneur')}</label>
                    </div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="business-owner" value="business-owner">
                        <label class="form-check-label" for="business-owner">{__('Business Owner')}</label>
                    </div><br/>
                    <div class="form-check" style="padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="other" value="other">
                        <label class="form-check-label" for="other">{__('Other')}</label>
                    </div>
                    <input type="text" class="form-control"/>
                </div>
                {*=================================================================================*}
                <div class="col-md-12" style="padding-bottom: 30px;">
                    <div>{__('Please Indicate Your Level Of Education *')}</div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="hight-school" value="hight-school">
                        <label class="form-check-label" for="hight-school">{__('Hight School')}</label>
                    </div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="college" value="college">
                        <label class="form-check-label" for="school-faculty">{__('College')}</label>
                    </div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="associate-degree" value="associate-degree">
                        <label class="form-check-label" for="associate-degree">{__('Associate’s Degree')}</label>
                    </div><br/>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="bachelor-degree" value="bachelor-degree">
                        <label class="form-check-label" for="bachelor-degree">{__('Bachelor’s Degree ')}</label>
                    </div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="master-degree" value="master-degree">
                        <label class="form-check-label" for="master-degree">{__('Master’s Degree')}</label>
                    </div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="doctorate-program" value="doctorate-program">
                        <label class="form-check-label" for="doctorate-program">{__('Doctorate Program')}</label>
                    </div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="PostDocs" value="PostDocs">
                        <label class="form-check-label" for="PostDocs">{__('PostDocs')}</label>
                    </div>
                    <div class="form-check" style="padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="other" value="other">
                        <label class="form-check-label" for="other">{__('Other')}</label>
                    </div>
                    <input type="text" class="form-control"/>
                </div>
                {*=========================================================================*}
                <div class="col-md-12" style="padding-bottom: 30px;">
                    <div>{__('Do you have any experience working as a freelancer?')}</div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="yes" value="yes">
                        <label class="form-check-label" for="yes">{__('Yes')}</label>
                    </div>
                    <div class="form-check" style="display: inline-block;padding-right: 40px;">
                        <input class="form-check-input" type="radio" name="radio" id="no" value="no">
                        <label class="form-check-label" for="no">{__('No')}</label>
                    </div>
                </div>
            </div>
            <div style="text-align: center;padding: 25px;">
                <button class="btn btn-primary">{__('Submit Form')}</button>
            </div>
        </div>   
    </div>
    {component name="/social_networks/SocialNetworksDisplay" tpl="landing"}
</section>


