            {*navigationText: ["<img src='myprevimage.png'>","<img src='mynextimage.png'>"],*}
        });
        
        function getFilterParameters()
        {
            var params={ filter: {  order : { }, 
                                    search : { },
                                    equal: {   
                                        lang : $(".Filter.Equal[name=lang]").attr('data-selected')
                                    }, 
                                    range : { },                                
                                nbitemsbypage: $("[name=nbitemsbypage]").val(),
                                token:'{$formFilter->getCSRFToken()}'
                         } };                
            return params;                  
        }
        
        function updateFilter()
        {          
           return $.ajax2({ data: getFilterParameters(), 
                            url:"{url_to('employees_ajax',['action'=>'ListPartialCategories'])}" ,                          
                            target: "#page-wrapper"
                             });
        }
        
        $(".Filter.Equal").click(function () {
             $(".Filter.Equal.Language").removeClass('active');             
             $(this).addClass('active').parent().parent().attr('data-selected',$(this).attr('data-value'));
             return updateFilter();
        });
        
        $(".pager").click(function () {                     
                return $.ajax2({ data: getFilterParameters(), 
                                 url:"{url_to('employees_ajax',['action'=>'ListPartialCategories'])}?page="+$(this).attr('data-page'),                                
                                 target: "#page-wrapper"
                });
        });
        
    {/JqueryScriptsReady}  
    </script>          